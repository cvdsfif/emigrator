{
  "name": "emigrator-ts",
  "version": "5.0.0-beta.34",
  "description": "Extendable database migration library adapted to CDK projects written in TypeScript",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "module": "./dist/index.mjs"
    },
    "./test": {
      "require": "./dist/test.js",
      "import": "./dist/test.js",
      "types": "./dist/test.d.ts",
      "module": "./dist/test.mjs"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsup ./src",
    "test": "jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cvdsfif/emigrator.git"
  },
  "keywords": [
    "dbmigrate",
    "cdk",
    "db-migrate",
    "migrate",
    "migration",
    "database",
    "sql",
    "postresql",
    "typescript",
    "lambda",
    "custom"
  ],
  "author": "Nikit Zykov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cvdsfif/emigrator/issues"
  },
  "homepage": "https://github.com/cvdsfif/emigrator#readme",
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.100.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.100.0-alpha.0",
    "@testcontainers/postgresql": "^10.2.1",
    "@types/aws-lambda": "^8.10.121",
    "@types/jest": "^29.5.4",
    "@types/pg": "^8.10.2",
    "aws-cdk-lib": "^2.114.1",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.2.70",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.0",
    "data-api-client": "^1.3.0",
    "esbuild": "^0.19.8",
    "jest": "^29.7.0",
    "pepelaz": "^5.0.0-beta.1",
    "pepelaz-db": "^5.0.0-beta.2",
    "pepelaz-test": "^5.0.0-beta.4",
    "pg": "^8.11.3",
    "stream": "^0.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
